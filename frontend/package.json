    "core-js": "^3.6.5",
    "env-dot-prop": "^3.0.0",
    "es6-promise": "^4.2.8",
    "http-server": "^0.12.3",
    "vue": "^3.2.41",
    "vue-axios": "^3.2.4",
    "vue-loader": "^16.3.3",
    "vue-router": "^4.0.10",
